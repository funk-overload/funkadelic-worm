{
    "compileOnSave": true,
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "src/javascripts",
        "target": "es5"
    },
    "include": [
        "src/typescripts/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}